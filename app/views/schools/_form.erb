<%= form_for @school do |f| %>
  <%= f.label :species1, "Fish" %>
  <%= f.select(:species1, @options)  %><br><br>
  <%= f.label :species2, "Fish" %>
  <%= f.select(:species2, @options)  %><br><br>
  <%= f.label :species3, "Fish" %>
  <%= f.select(:species3, @options)  %><br><br>
  <%= f.hidden_field :aquarium_id, :value => @aquarium.id %>
  <%= f.submit "Create" %><br>
<% end %>
